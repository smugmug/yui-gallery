{
    "gallery-sm-editor": {
        "requires": [
            "gallery-sm-editor-base",
            "gallery-sm-editor-keys",
            "gallery-sm-editor-link",
            "gallery-sm-editor-queue",
            "gallery-sm-editor-style",
            "gallery-sm-editor-undo"
        ],

        "skinnable": true
    },

    "gallery-sm-editor-base": {
        "requires": [
            "base-build",
            "classnamemanager",
            "event-focus",
            "gallery-sm-selection",
            "view"
        ]
    },

    "gallery-sm-editor-dom": {
        "requires": [
            "gallery-sm-range",
            "node"
        ]
    },

    "gallery-sm-editor-keys": {
        "requires": [
            "gallery-sm-editor-base"
        ]
    },

    "gallery-sm-editor-link": {
        "requires": [
            "gallery-sm-editor-base",
            "gallery-sm-editor-dom"
        ]
    },

    "gallery-sm-editor-queue": {
        "requires": [
            "gallery-sm-editor-base",
            "gallery-sm-editor-keys"
        ]
    },

    "gallery-sm-editor-style": {
        "requires": [
            "gallery-sm-editor-base",
            "gallery-sm-editor-dom",
            "gallery-sm-editor-keys",
            "gallery-sm-editor-queue",
            "node-style"
        ]
    },

    "gallery-sm-editor-undo": {
        "requires": [
            "gallery-sm-editor-base"
        ]
    }
}
